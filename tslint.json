{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*.ts"]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "variable-name": [true, "allow-leading-underscore"]
  },
  "jsRules": {
    "object-literal-sort-keys": false
  }
}
